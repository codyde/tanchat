#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/codydearkland/Downloads/tanchat/node_modules/listhen/bin/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/listhen/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules:/Users/codydearkland/Downloads/node_modules:/Users/codydearkland/node_modules:/Users/node_modules:/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/codydearkland/Downloads/tanchat/node_modules/listhen/bin/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/listhen/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules:/Users/codydearkland/Downloads/node_modules:/Users/codydearkland/node_modules:/Users/node_modules:/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../listhen/bin/listhen.mjs" "$@"
else
  exec node  "$basedir/../listhen/bin/listhen.mjs" "$@"
fi
