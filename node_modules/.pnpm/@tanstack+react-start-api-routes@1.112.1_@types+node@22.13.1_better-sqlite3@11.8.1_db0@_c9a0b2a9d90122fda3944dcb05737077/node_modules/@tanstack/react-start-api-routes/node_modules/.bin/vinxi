#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules/vinxi/bin/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules/vinxi/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules/vinxi/bin/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules/vinxi/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules:/Users/codydearkland/Downloads/tanchat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules/vinxi/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vinxi@0.5.3_@types+node@22.13.1_better-sqlite3@11.8.1_db0@0.2.3_better-sqlite3@11.8.1___fd7e186e33707ca26b838347ca57817c/node_modules/vinxi/bin/cli.mjs" "$@"
fi
